# Simple Environment Test

Say "Testing environment variables..."

# Test basic functionality
Say "Testing get_env with default:"
Let test_var be get_env("NONEXISTENT_VAR", "default_value")
Say "Result: " + test_var

Say "Testing has_env:"
Let has_path be has_env("PATH")
Say "Has PATH: " + to_text(has_path)

Say "Testing set_env:"
Call set_env with "CONVO_TEST" and "Hello World"
Let retrieved be get_env("CONVO_TEST")
Say "Set and retrieved: " + retrieved

Say "Environment test complete!"
